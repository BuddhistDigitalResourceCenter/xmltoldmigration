	<xsd:complexType name="WfAuditTrail">
		<xsd:sequence>
			<xsd:element name="step" type="c:Step" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Step">
		<xsd:sequence>
			<xsd:element name="note" type="c:Note" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
